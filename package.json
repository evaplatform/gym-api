{
  "name": "gym-api",
  "version": "1.0.0",
  "main": "dist/index.js",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "start": "node dist/index.js",
    "build": "tsc --project tsconfig.json && tsc-alias",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "vercel-build": "tsc && tsc-alias"
  },
  "_moduleAliases": {
    "@": "dist"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.782.0",
    "axios": "^1.8.4",
    "bcryptjs": "^3.0.2",
    "dotenv": "^16.4.7",
    "express": "^5.1.0",
    "glob": "^11.0.3",
    "google-auth-library": "^9.15.1",
    "jose": "^6.0.11",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "mongoose": "^8.13.2",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.0.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/module-alias": "^2.0.4",
    "@types/multer": "^1.4.12",
    "@types/multer-s3": "^3.0.3",
    "@types/node": "^24.3.1",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  }
}